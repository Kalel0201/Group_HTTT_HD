@using WebApp.Resources
@model IEnumerable<DTO.Publisher>
@{
    ViewBag.Title = "Publisher";
}
<h2>@Language.ManagePublisher</h2>
<p>
    @Html.ActionLink(Language.CreateNew, "Create")
</p>
<table class="table">
    <tr><th>@Language.PublisherName</th><th class="colsmall">@Language.Edit</th><th class="colsmall">@Language.Delete</th></tr>
@foreach (var item in Model)
{
    <tr>
		<td>@item.PublisherName</td>

        <td><a href="/Publisher/Edit/@item.PublisherId"><img src="/Images/Edit.gif" alt="Edit" /></a></td>
        <td><a href="/Publisher/Delete/@item.PublisherId"><img src="/Images/Delete.gif" alt="Delete" /></a></td>
    </tr>
}
</table>
