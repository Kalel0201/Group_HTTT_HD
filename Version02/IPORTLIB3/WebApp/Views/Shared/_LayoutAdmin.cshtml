<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
	<link href="~/Contents/bootstrap.min.css" rel="stylesheet" />
	<link href="~/Contents/font-awesome-4.1.0/css/font-awesome.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="/Images/jquery-ui.css">
	<link href="/Images/Style.css" rel="stylesheet" type="text/css" />
</head>
<body>
    @{ Html.RenderPartial("~/Views/Shared/_PartialTop.cshtml");}
    <div id="main" class="wrapper">
        <div id="left">
            @{Html.RenderAction("Index", "Menu");}
        </div>
        <div id="right">
            @{Html.RenderAction("Breadcrumb", "Menu", new { id = ViewBag.Title });}
            @RenderBody()
        </div>
        <div class="cls"></div>
    </div>
    <div id="footer">
        <div id="footer-copyright">
            <div class="wrapper">Coppyright &copy; Group 07</div>
        </div>
    </div>
	<script src="~/Images/jquery-1.10.2.min.js" type="text/javascript"></script>
	<script src="~/Images/bootstrap.min.js" type="text/javascript"></script>
    <script src="/Images/highcharts.js" type="text/javascript"></script>
    <script src="/Images/exporting.js" type="text/javascript"></script>
    <script src="/Images/highcharts-3d.js" type="text/javascript"></script>
    <script src="/Images/jquery-ui.js" type="text/javascript"></script>
    <script src="~/Images/App.js" type="text/javascript"></script>
</body>
</html>