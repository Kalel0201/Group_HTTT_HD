@using WebApp.Resources
@model IEnumerable<DTO.Supplier>
@{
    ViewBag.Title = "Supplier";
}
<h2>@Language.ManageSupplier</h2>
<p>
    @Html.ActionLink(Language.CreateNew, "Create")
</p>
<table class="table">
    <tr><th>@Language.SupplierName</th><th class="colsmall">@Language.Edit</th><th class="colsmall">@Language.Delete</th></tr>
@foreach (var item in Model)
{
    <tr>
		<td>@item.SupplierName</td>

        <td><a href="/Supplier/Edit/@item.SupplierId"><img src="/Images/Edit.gif" alt="Edit" /></a></td>
        <td><a href="/Supplier/Delete/@item.SupplierId"><img src="/Images/Delete.gif" alt="Delete" /></a></td>
    </tr>
}
</table>
